{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Current TS File",
      "runtimeExecutable": "node",
      // 可在 process.execArgv 中获取
      "runtimeArgs": ["-r", "ts-node/register"],
      // 可在 process.argv 中获取
      "args": ["${relativeFile}"],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "protocol": "inspector",
      "env": {
        "dev": "true"
      }
    }
  ]
}
